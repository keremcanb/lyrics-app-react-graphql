{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "object-curly-newline": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off"
    // "array-callback-return": "off",
  }
}
